    <filter-name>FilterOutput</filter-name>
    <url-pattern>/FilterOutput</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>OutputFilter</display-name>
    <filter-name>OutputFilter</filter-name>
    <filter-class>OutputFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>OutputFilter</filter-name>
    <url-pattern>/OutputFilter</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>FilterOutput1</display-name>
    <filter-name>FilterOutput1</filter-name>
    <filter-class>FilterOutput1</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterOutput1</filter-name>
    <url-pattern>/FilterOutput1</url-pattern>
  </filter-mapping>
</web-app>